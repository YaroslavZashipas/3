import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        double a = sc.nextDouble();
        double b = sc.nextDouble();
        double x = sc.nextDouble();

        double fx;

        if (x < 4) {
            fx = Math.cos(x);
        } else if (x == 4) {
            fx = x * x * x - b * x + 1;
        } else if (x > 4 && x < 7) {
            fx = Math.sqrt(a * x);
        } else {
            sc.close();
            return;
        }

        System.out.println(fx);
        sc.close();
    }
}
